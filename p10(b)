#include <stdio.h>
int main() {
    int n, original, reversed = 0, digit;
    printf("Enter a number: ");
    scanf("%d", &n);
    original = n;
    while(n > 0) {
        digit = n % 10;     
        reversed = reversed * 10 + digit; 
        n = n / 10;            
    }
    if(reversed == original)
        printf("The number is a palindrome.\n");
    else
        printf("The number is not a palindrome.\n");
    return 0;
}