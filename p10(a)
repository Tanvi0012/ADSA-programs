#include <stdio.h>
#include <string.h>
int main() {
    char str[20];
    int i, len, isPalindrome = 1;
    printf("Enter a number: ");
    scanf("%s", str); 
    len = strlen(str);
    for(i = 0; i < len / 2; i++) {
        if(str[i] != str[len - 1 - i]) {
            isPalindrome = 0;
            break;
        }
    }
    if(isPalindrome)
        printf("The number is a palindrome.\n");
    else
        printf("The number is not a palindrome.\n");
    return 0;
}