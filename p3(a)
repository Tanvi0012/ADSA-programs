#include <stdio.h>
#include <string.h>
int main() {
    char str[100];
    int left, right;
    printf("Enter username");
    scanf("%s", str);
    left = 0;
    right = strlen(str) - 1;
    
    while(left < right) {
        char temp = str[left];
        str[left] = str[right];
        str[right] = temp;
        left++;
        right--;
    }
    printf("Reversed username= %s", str);
    return 0;
}