#include <stdio.h>
#include <string.h>
int main() {
    char str[100], rev[100];
    int n, i;
    printf("Enter username:");
    scanf("%s", str);
    n = strlen(str);
    for(i = 0; i < n; i++) {
        rev[i] = str[n - 1 - i];
    }
    rev[n] = '\0';
    printf("Reversed username: %s", rev);
    return 0;
}