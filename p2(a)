// Find maximum element using a simple linear scan (O(n)).
#include<stdio.h>
int main(){
    int n,i,max;
    printf("enter no. of days");
    scanf("%d",&n);
    int temp[n];
    printf("enter temperature reading");
    for(i=0;i<n;i++) {
        scanf("%d",&temp[i]);
    }
    max=temp[0];
    for(i=1;i<n;i++) {
        if(temp[i]>max) {
            max=temp[i];
        }
    }
    printf("max temperature=%d",max);
    return 0;
}